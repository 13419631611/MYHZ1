+    <corners
+        android:radius="5dp"/>
+    <!-- 节点1：corners (圆角)
+        android:radius  圆角的半径 值越大角越圆
+        android:topLeftRadius   左上圆角半径
+        android:topRightRadius  右上圆角半径
+        android:bottomLeftRadius    左下圆角半径
